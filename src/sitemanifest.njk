---
permalink: /public/site.webmanifest
eleventyExcludeFromCollections: true
---

{
    "name": "{{ site.title }}",
    "short_name": "{{ site.title }}",
    "icons": [
        {
            "src": "{{ '/assets/favicons/android-chrome-192x192.png' | url }}",
            "sizes": "192x192",
            "type": "image/png"
        },
        {
            "src": "{{'/assets/favicons/android-chrome-256x256.png' | url }}",
            "sizes": "256x256",
            "type": "image/png"
        }
    ],
    "theme_color": "#ffffff",
    "background_color": "#ffffff",
    "display": "standalone"
}